@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Customer/Master.cshtml";
}
<marquee>Customer's Details</marquee>
<table border="1">
 <tr><th>Id</th><th>Name</th><th>EmploymentType</th><th>Salary</th><th>MobileNo</th><th>Area</th><th>SalaryReceivedIn</th>
    <th>DesiredLoanAmount</th><th>Gender</th><th>Pincode</th><th>PANNo</th><th>DOB</th><th>Email</th><th>Company</th>
    <th>CurrentEMIAmount</th><th>Tenure</th><th>Experience</th><th>HaveLoan?</th></tr>
    @foreach(var value in ViewData["customers"] as IEnumerable<Online_LoanEnquiry_And_Loan_Calculator_EL.Customer>)
    {
        <tr>
            <td>@value.id</td>
            <td>@value.name</td>
            <td>@value.type</td>
            <td>@value.monthlyIncome</td>
            <td>@value.mobilenumber</td>
            <td>@value.residentArea</td>
            <td>@value.salaryReceivedIn</td>
            <td>@value.desiredLoanAmount</td>
            <td>@value.gender</td>
            <td>@value.pincode</td>
            <td>@value.panCardNumber</td>
            <td>@value.dateOfBirth</td>
            <td>@value.email</td>
            <td>@value.company</td>
            <td>@value.currentEMIAmount</td>
            <td>@value.tenure</td>
            <td>@value.experience</td>
            <td>@value.haveLoan</td>
        </tr>
    }
</table>


